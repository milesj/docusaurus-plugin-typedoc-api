{
  "name": "docusaurus-plugin-typedoc-api",
  "version": "3.0.0",
  "description": "Docusaurus plugin that provides source code API documentation powered by TypeDoc. ",
  "keywords": [
    "docusaurus",
    "plugin",
    "typedoc",
    "api"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:milesj/docusaurus-plugin-typedoc-api.git"
  },
  "author": "Miles Johnson",
  "license": "MIT",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "assets/**/*",
    "lib/**/*",
    "src/**/*",
    "styles.css"
  ],
  "engines": {
    "node": ">=16.12.0"
  },
  "funding": {
    "type": "ko-fi",
    "url": "https://ko-fi.com/milesjohnson"
  },
  "packemon": {
    "format": "lib",
    "platform": "node",
    "externals": [
      "@theme/*",
      "@docusaurus/*",
      "@vscode/*",
      "css$"
    ]
  },
  "peerDependencies": {
    "@docusaurus/core": "^2.0.0",
    "react": ">=16.0.0",
    "typescript": "^4.0.0 || ^5.0.0"
  },
  "dependencies": {
    "@docusaurus/plugin-content-docs": "^2.4.1",
    "@docusaurus/types": "^2.4.1",
    "@docusaurus/utils": "^2.4.1",
    "@vscode/codicons": "^0.0.32",
    "marked": "^4.3.0",
    "typedoc": "^0.23.28"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^2.4.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "typescript": "^5.0.2"
  }
}
