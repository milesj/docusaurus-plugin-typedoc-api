{
  "name": "docusaurus-plugin-typedoc-api",
  "version": "4.1.0",
  "description": "Docusaurus plugin that provides source code API documentation powered by TypeDoc. ",
  "keywords": [
    "docusaurus",
    "plugin",
    "typedoc",
    "api"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:milesj/docusaurus-plugin-typedoc-api.git"
  },
  "author": "Miles Johnson",
  "license": "MIT",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "assets/**/*",
    "lib/**/*",
    "src/**/*",
    "styles.css"
  ],
  "engines": {
    "node": ">=16.12.0"
  },
  "funding": {
    "type": "ko-fi",
    "url": "https://ko-fi.com/milesjohnson"
  },
  "packemon": {
    "format": "lib",
    "platform": "node",
    "externals": [
      "@theme/*",
      "@docusaurus/*",
      "@vscode/*",
      "css$"
    ]
  },
  "peerDependencies": {
    "@docusaurus/core": "^3.0.0",
    "react": ">=18.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@docusaurus/plugin-content-docs": "^3.1.0",
    "@docusaurus/types": "^3.1.0",
    "@docusaurus/utils": "^3.1.0",
    "@vscode/codicons": "^0.0.35",
    "marked": "^9.1.6",
    "marked-smartypants": "^1.1.5",
    "typedoc": "^0.25.7"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^3.1.0",
    "@docusaurus/tsconfig": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.3.3"
  }
}
