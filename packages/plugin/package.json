{
  "name": "docusaurus-plugin-typedoc-api",
  "version": "1.10.0",
  "description": "Docusaurus plugin that provides source code API documentation powered by TypeDoc. ",
  "keywords": [
    "docusaurus",
    "plugin",
    "typedoc",
    "api"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:milesj/docusaurus-plugin-typedoc-api.git"
  },
  "author": "Miles Johnson",
  "license": "MIT",
  "main": "./lib/index.js",
  "types": "./dts/index.d.ts",
  "files": [
    "assets/**/*",
    "dts/**/*.d.ts",
    "lib/**/*.{js,map}",
    "src/**/*.{ts,tsx,json}",
    "styles.css"
  ],
  "engines": {
    "node": ">=12.17.0",
    "npm": ">=6.13.0"
  },
  "funding": {
    "type": "ko-fi",
    "url": "https://ko-fi.com/milesjohnson"
  },
  "packemon": {
    "format": "lib",
    "platform": "node",
    "externals": [
      "@theme/*",
      "@docusaurus/*",
      "@vscode/*",
      "css$"
    ]
  },
  "peerDependencies": {
    "react": ">=16.0.0",
    "typescript": "^4.0.0"
  },
  "dependencies": {
    "@docusaurus/plugin-content-docs": "2.0.0-beta.19",
    "@docusaurus/types": "2.0.0-beta.19",
    "@docusaurus/utils": "2.0.0-beta.19",
    "@vscode/codicons": "^0.0.29",
    "marked": "^3.0.8",
    "typedoc": "^0.22.15"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "2.0.0-beta.19",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "typescript": "^4.6.4"
  }
}
